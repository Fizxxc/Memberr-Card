{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/register",
      "dest": "/public/register.html"
    },
    {
      "src": "/user",
      "dest": "/public/user-dashboard.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
